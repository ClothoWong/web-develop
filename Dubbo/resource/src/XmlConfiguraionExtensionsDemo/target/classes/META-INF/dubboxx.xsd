<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://www.evada.com/schema/dubboxx"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:beans="http://www.springframework.org/schema/beans"
            targetNamespace="http://www.evada.com/schema/dubboxx"
            elementFormDefault="qualified"
            attributeFormDefault="unqualified">
    <xsd:import namespace="http://www.springframework.org/schema/beans"/>
    <xsd:element name="admin">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="beans:identifiedType">
                    <xsd:attribute name="uname" type="xsd:string" use="required"/>
                    <xsd:attribute name="age" type="xsd:string" use="required"/>
                    <xsd:attribute name="sex" type="xsd:string" use="required"/>
                    <xsd:attribute name="enable" type="xsd:boolean"/>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>